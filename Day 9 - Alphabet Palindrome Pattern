
#include <iostream>
using namespace std;

int main() {
    int N;
    cout << "Enter the number of rows for the Palindromic Alphabet Pattern: ";
    cin >> N;

    for (int i = 0; i < N; i++) {
        char ch = 'A';
        for (int j = 0; j <= i; j++) {
            cout << ch++;
        }
        ch--;
        for (int j = 0; j < i; j++) {
            cout << --ch;
        }
        cout << endl;
    }

    return 0;
}
