
#include <iostream>

using namespace std;

int main() {
    int arr[5];
    int product = 1;

    cout << "Enter 5 integers:" << endl;

    for (int i = 0; i < 5; i++) {
        cin >> arr[i];
        product *= arr[i];
    }

    cout << "Product of the elements in the array: " << product << endl;

    return 0;
}
